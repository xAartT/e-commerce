# Porta do servidor
PORT=5000

# URL de conexão do PostgreSQL
# Formato: postgresql://usuario:senha@host:porta/nome_do_banco
DATABASE_URL=postgresql://postgres:senha@localhost:5432/ecommerce

# Secret para geração de tokens JWT (use uma string aleatória e segura)
JWT_SECRET=seu_secret_super_seguro_aqui_troque_por_valor_aleatorio

# Ambiente (development, production)
NODE_ENV=development